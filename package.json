{
  "name": "angular-nestjs-render",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "concurrently --kill-others \"npm run server:watch\" \"ng serve --aot --progress=false --proxy-config proxy.conf.json\"",
    "client:build": "ng build --prod",
    "client:lint": "ng lint --fix",
    "server:lint": "tslint  --project ./tsconfig.json './src/server/**/*.ts' -c tslint.server.json --fix",
    "server:watch": "nodemon",
    "build:universal": "npm run helper:build:client-and-server && npm run helper:webpack:nest",
    "serve:universal": "node dist/server.js",
    "helper:build:client-and-server": "ng build --prod && ng build --prod --app 1 --output-hashing=false",
    "helper:webpack:nest": "webpack --config webpack.server.config.js --colors",
    "prettify": "pretty-quick"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.9",
    "@angular/common": "^9.1.9",
    "@angular/compiler": "^9.1.9",
    "@angular/core": "^9.1.9",
    "@angular/forms": "^9.1.9",
    "@angular/platform-browser": "^9.1.9",
    "@angular/platform-browser-dynamic": "^9.1.9",
    "@angular/platform-server": "^9.1.9",
    "@angular/router": "^9.1.9",
    "@nestjs/common": "^7.1.0",
    "@nestjs/core": "^7.1.0",
    "@nestjs/microservices": "^7.1.0",
    "@nestjs/testing": "^7.1.0",
    "@nestjs/websockets": "^7.1.0",
    "@nguniversal/module-map-ngfactory-loader": "^8.2.6",
    "copy-webpack-plugin": "^6.0.1",
    "core-js": "^3.6.5",
    "dotenv": "^8.2.0",
    "ng-socket-io": "^0.2.4",
    "rxjs": "^6.5.5",
    "tslib": "^2.0.0",
    "webpack-node-externals": "^1.7.2",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular/cli": "^9.1.7",
    "@angular/compiler-cli": "^9.1.9",
    "@angular/language-service": "^9.1.9",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.6",
    "@types/node": "^14.0.5",
    "codelyzer": "^5.2.2",
    "concurrently": "^5.2.0",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "protractor": "^7.0.0",
    "ts-loader": "^7.0.5",
    "ts-node": "^8.10.1",
    "tslint": "^6.1.2",
    "tslint-sonarts": "^1.9.0",
    "typescript": "~3.8.3"
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 140
  }
}
